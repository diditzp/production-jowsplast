                            "label": "",
                            "id": "field16",
                            "value": "<script>\n    // Handling changes in the 'sku_produk' input field\n    $(document).ready(function (){\n        $(\"[name=sku_produk]\").change(function () {\n            var item = $(this).val(); // Get the value from input field\n            console.log(\"apaaaa 0-->\"+ item);\n            getDetailItems(item); // Call function to fetch details\n        });\n        \n        $('[name=input_manual]').change(function() {\n            var item = $(this).val();\n            console.log('input manual : ', item);\n            getDetailItems(item);\n        })\n        \n        // Function to send AJAX request to fetch product details\n        function getDetailItems(item) {\n            console.log(\"hasil ws qeury 1-->\"+nama_sku);\n            console.log(\"hasil ws qeury 2-->\"+item);\n            $.ajax({\n                url: \"#request.contextPath#/web/json/plugin/org.joget.jsp.GetDetailProduk/service?\",\n                type: \"GET\",\n                data: { sku_produk: item },\n                dataType: \"json\",\n                success: function (res) {\n                    // Assuming 'res.nama_sku' is the desired value\n                    const nama_sku = res.nama_sku;\n                    const sisa_qty = res.qty;\n                    const sisa_g_produksi = res.gudang_produksi;\n                    const sisa_g_packing = res.gudang_packing;\n                    \n                    $(\"[name=nama_sku]\").val(nama_sku); // Set the value in the corresponding field\n                    $(\"[name=sisa_stock]\").val(sisa_qty); // Set the value in the corresponding field\n                    $(\"[name=gudang_produksi]\").val(sisa_g_produksi); // Set the value in the corresponding field\n                    $(\"[name=gudang_packing]\").val(sisa_g_packing); // Set the value in the corresponding field\n                    console.log(\"hasil ws qeury 3-->\"+nama_sku);\n                    console.log(\"hasil ws qeury 3-->\"+sisa_qty);\n                    console.log(\"hasil ws qeury 3-->\"+sisa_g_produksi);\n                    console.log(\"hasil ws qeury 3-->\"+sisa_g_packing);\n                },\n                error: function (xhr, status, error) {\n                    console.log(\"Error: \" + xhr.responseText);\n                }\n            });\n            \n        }\n    })\n<\/script>"
                        }
