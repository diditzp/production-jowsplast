    "body": {"elements": [{
        "className": "org.joget.rbuilder.lib.HtmlReportElement",
        "properties": {
            "html": "<style type=\"text/css\">\r\n    #container {\r\n        width: 200px;\r\n        height: 200px;\r\n        display: none;\r\n    }\r\n    #printOptions {\r\n        margin-top: 10px;\r\n        display: none;\r\n    }\r\n    .print-button {\r\n        cursor: pointer;\r\n        padding: 5px 10px;\r\n        border: 1px solid #ccc;\r\n        background-color: #f0f0f0;\r\n        margin-right: 5px;\r\n        border-radius: 5px;\r\n    }\r\n    #container-data {\r\n        display: flex;\r\n    }\r\n<\/style>\r\n\r\n<script type=\"text/javascript\" src=\"#appResource.qrcode.min.js#\"><\/script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/canvg/3.0.7/umd.min.js\"><\/script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jspdf/2.5.1/jspdf.umd.min.js\"><\/script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jspdf-autotable/3.5.13/jspdf.plugin.autotable.min.js\"><\/script>\r\n\r\n<script type=\"text/javascript\">\r\n$(function() {\r\n    function generateQRCode() {\r\n        var namaProduk = $('[name=nama_produk] option:selected').text();\r\n        var requestProduksi = $('[name=requestProduksi]').val();\r\n        var totalAktual = $('[name=totalAktual]').val();\r\n        var tanggalProduksi = $('[name=tanggal_produksi]').val();\r\n        var shift = $('input[name=\"shift\"]:checked').parent().text().trim();\r\n        var id = \"#form.produksi.nama_produk#\";\r\n\r\n        $(\"#productName\").text(namaProduk);\r\n        $(\"#requestProduction\").text(requestProduksi);\r\n        $(\"#totalActual\").text(totalAktual);\r\n        $(\"#productionDate\").text(tanggalProduksi);\r\n        $(\"#shiftValue\").text(shift);\r\n        \r\n        $(\"#additionalData\").show();\r\n\r\n        if (!id.startsWith(\"#\")) {\r\n            $(\"#container\").show();\r\n            $(\"#printOptions\").show();\r\n            \r\n            var qrcode = new QRCode({\r\n                content: id,\r\n                container: \"svg-viewbox\",\r\n                join: true\r\n            });\r\n            var svg = qrcode.svg();\r\n            document.getElementById(\"container\").innerHTML = svg;\r\n            document.getElementById(\"containerExplain\").innerHTML = \"Scan this QR Code\";\r\n            document.getElementById(\"containerText\").innerHTML = namaProduk;\r\n        }\r\n    }\r\n\r\n    generateQRCode();\r\n});\r\n\r\n// Function to print QR code as PDF\r\n// function printPDF() {\r\n//     var { jsPDF } = window.jspdf;\r\n//     var doc = new jsPDF();\r\n    \r\n//     // Get the SVG element\r\n//     var svgElement = document.getElementById('container').querySelector('svg');\r\n\r\n//     // Create a canvas to render the SVG to\r\n//     var canvas = document.createElement('canvas');\r\n//     canvas.width = svgElement.clientWidth;\r\n//     canvas.height = svgElement.clientHeight;\r\n    \r\n//     // Use canvg to draw the SVG onto the canvas\r\n//     var ctx = canvas.getContext('2d');\r\n//     canvg.Canvg.fromString(ctx, new XMLSerializer().serializeToString(svgElement)).start();\r\n\r\n//     // Convert the canvas to a PNG image\r\n//     var imgData = canvas.toDataURL('image/png');\r\n    \r\n//     // Add the image and text to the PDF\r\n//     doc.addImage(imgData, 'PNG', 15, 40, 180, 160);\r\n//     doc.text(20, 30, document.getElementById(\"containerText\").innerHTML);\r\n    \r\n//     var saveFile = 'QR-code-'+document.getElementById(\"containerText\").innerHTML;\r\n//     // Save the PDF\r\n//     doc.save(saveFile);\r\n// }\r\nfunction printPDF() {\r\n    var { jsPDF } = window.jspdf;\r\n    var doc = new jsPDF();\r\n    \r\n    // Get the SVG element\r\n    var svgElement = document.getElementById('container').querySelector('svg');\r\n\r\n    // Create a canvas to render the SVG to\r\n    var canvas = document.createElement('canvas');\r\n    canvas.width = svgElement.clientWidth;\r\n    canvas.height = svgElement.clientHeight;\r\n    \r\n    // Use canvg to draw the SVG onto the canvas\r\n    var ctx = canvas.getContext('2d');\r\n    canvg.Canvg.fromString(ctx, new XMLSerializer().serializeToString(svgElement)).start();\r\n\r\n    // Convert the canvas to a PNG image\r\n    var imgData = canvas.toDataURL('image/png');\r\n    \r\n    // Add the image and text to the PDF\r\n    doc.addImage(imgData, 'PNG', 15, 40, 60, 60); // Adjust size and position as needed\r\n\r\n    // Define table columns and rows\r\n    const tableColumn = [\"Produksi\", \"Tanggal\", \"Shift\"];\r\n    const tableRows = [\r\n        [document.getElementById(\"productName\").innerHTML],\r\n        [document.getElementById(\"requestProduction\").innerHTML],\r\n        [\r\n            document.getElementById(\"totalActual\").innerHTML,\r\n            document.getElementById(\"productionDate\").innerHTML,\r\n            document.getElementById(\"shiftValue\").innerHTML\r\n        ]\r\n    ];\r\n\r\n    // Define table styles\r\n    doc.autoTable({\r\n        head: [tableColumn],\r\n        body: tableRows,\r\n        startY: 100, // Adjust start position for the table\r\n        margin: { left: 15, right: 15 },\r\n        styles: {\r\n            cellPadding: 5,\r\n            fontSize: 12,\r\n            overflow: 'linebreak',\r\n            tableLineColor: [0, 0, 0],\r\n            tableLineWidth: 0.75,\r\n            halign: 'center', // Center align text in each cell\r\n            valign: 'middle', // Center align vertically\r\n        },\r\n        headStyles: {\r\n            fillColor: [220, 220, 220], // Light gray header\r\n            textColor: [0, 0, 0], // Black text\r\n            fontStyle: 'bold',\r\n        },\r\n        didParseCell: function(data) {\r\n            // Apply specific styles to the header or specific cells if needed\r\n        },\r\n    });\r\n\r\n    // Add the product name above the table\r\n    doc.text(20, 90, document.getElementById(\"containerText\").innerHTML); // Adjust position as needed\r\n    \r\n    var saveFile = 'QR-code-' + document.getElementById(\"containerText\").innerHTML;\r\n    // Save the PDF\r\n    doc.save(saveFile);\r\n}\r\n<\/script>\r\n\r\n<!-- Print Options -->\r\n<div id=\"printOptions\">\r\n    <span class=\"print-button\" onclick=\"printPDF()\">Print as PDF<\/span>\r\n<\/div>\r\n\r\n<div id=\"containerExplain\"><\/div>\r\n<div id=\"container-data\">\r\n    <div id=\"container\"><\/div>\r\n    <div id=\"additionalData\" style=\"margin-top: 20px; display: none;\">\r\n        <table border=\"1\" cellpadding=\"5\" style=\"width: 150%; height: 88%\">\r\n            <tbody style=\"text-align: center;\">\r\n                <tr>\r\n                    <td id=\"productName\" colspan=\"3\" style=\"font-weight: bold;\"><\/td>\r\n                <\/tr>\r\n                <tr>\r\n                    <td id=\"requestProduction\" colspan=\"3\" style=\"font-weight: bold;\"><\/td>\r\n                <\/tr>\r\n                <tr>\r\n                    <th>Produksi<\/th>\r\n                    <th>Tanggal<\/th>\r\n                    <th>Shift<\/th>\r\n                <\/tr>\r\n                <tr>\r\n                    <td id=\"totalActual\"><\/td>\r\n                    <td id=\"productionDate\"><\/td>\r\n                    <td id=\"shiftValue\"><\/td>\r\n                <\/tr>\r\n            <\/tbody>\r\n        <\/table>\r\n    <\/div> \r\n<\/div>\r\n<div id=\"containerText\"><\/div>",
